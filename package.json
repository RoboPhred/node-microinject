{
  "name": "microinject",
  "version": "0.1.0",
  "description": "A Tiny, standalone, library-friendly dependency injection.",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "clean": "rimraf lib",
    "prebuild": "npm run clean",
    "build": "tsc -p src",
    "watch:build": "tsc -p src -w",
    "watch:test": "mocha --require ./mocha.env.js --watch ./lib/**/*.spec.js",
    "watch": "concurrently \"npm run watch:build\" \"npm run watch:test\"",
    "test": "mocha --require ./mocha.env.js ./lib/**.spec.js",
    "examples:clean": "rimraf examples/lib",
    "preexamples:build": "npm run examples:clean",
    "examples:build": "tsc -p examples"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/RoboPhred/node-microinject.git"
  },
  "keywords": [
    "ioc",
    "inversion of control",
    "di",
    "dependency injection",
    "node"
  ],
  "author": "William Matthews",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/RoboPhred/node-microinject/issues"
  },
  "devDependencies": {
    "@types/chai": "^4.1.2",
    "@types/mocha": "^5.0.0",
    "@types/sinon": "^4.3.0",
    "@types/uuid": "^3.4.3",
    "chai": "^4.1.2",
    "concurrently": "^3.5.1",
    "mocha": "^5.0.5",
    "rimraf": "^2.6.2",
    "sinon": "^4.4.8",
    "source-map-support": "^0.5.4",
    "tslint": "^5.9.1",
    "typescript": "^2.7.2"
  },
  "engines": {
    "node": ">=6.13.1"
  },
  "dependencies": {
    "uuid": "^3.2.1"
  }
}
